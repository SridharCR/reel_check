{
  "name": "reelcheck-extension",
  "version": "1.0.0",
  "description": "Chrome extension for ReelCheck fact-checking tool.",
    "manifest_version": 3,
  "permissions": [
    "debugger",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}